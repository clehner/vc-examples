{
  "@context": {
    "@protected": true,

    "FoodSafetyCertificationCredential": {
      "@id": "https://credential-handler.github.io/vc-examples/safechef/#FoodSafetyCertificationCredential",
      "@context": {
        "name": "http://schema.org/name",
        "description": "http://schema.org/description",
        "image": {
          "@id": "http://schema.org/image",
          "@type": "@id"
        },

        "certificateNo": {
          "@id": "https://credential-handler.github.io/vc-examples/safechef/#certificateNo",
          "@type": "xsd:string"
        },
        "examDate": {
          "@id": "https://credential-handler.github.io/vc-examples/safechef/#examDate",
          "@type": "xsd:dateTime"
        },
        "testCode": {
          "@id": "https://credential-handler.github.io/vc-examples/safechef/#testCode",
          "@type": "xsd:string"
        }
      }
    }
  }
}
